{
    "package": {
        "name": "hello-world-go",
        "repo": "bin",
        "subject": "suntong",
        "desc": "A simple Go application on Linux.",
        "website_url": "https://github.com/suntong/hello-world-go",
        "issue_tracker_url": "https://github.com/suntong/hello-world-go/issues",
        "vcs_url": "https://github.com/suntong/hello-world-go.git",
        "licenses": ["Apache-2.0"],
        "public_download_numbers": false
    },

    "version": {
        "name": "latest",
        "gpgSign": false
    },

    "files": [{
            "includePattern": "/home/travis/gopath/bin/hello-([ld].*)-amd64", 
            "uploadPattern": "/hello-world-go/static-amd64-$1/hello",
            "matrixParams": {
                "override": 1 
            }
        },
        {
            "includePattern": "/home/travis/gopath/bin/hello-windows-amd64", 
            "uploadPattern": "/hello-world-go/static-amd64-windows/hello.exe",
            "matrixParams": {
                "override": 1 
            }
        }],

    "publish": true
}
